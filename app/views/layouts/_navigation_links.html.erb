<% if !user_signed_in? %>
<li><%= link_to "Sign in", new_user_session_path %></li>
<li><%= link_to "Sign up", new_user_registration_path %></li>
<% else %>
<% if current_user.profile.firstname.nil?%>
<li class="pull-left"><%= link_to "Profile", edit_user_profiles_path(current_user.id)%></li>
<% else %>
<li class="pull-left"><%= link_to "Profile", user_profiles_path(current_user.id)%></li>
<%end%>
<li class="pull-left"><%= link_to "Properties", properties_path%></li>
<li><%= link_to "Edit your account", edit_user_registration_path %></li>

<% if current_user.admin? %>
<li><%= link_to "Admin", admin_root_path %></li>
<% end %>

<li><%= link_to "Sign out", destroy_user_session_path, data:{method: :delete} %></li>
<% end %>